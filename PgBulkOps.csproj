<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net9.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>

    <PackageId>PgBulkOps</PackageId>
    <Version>0.1.0</Version>
    <Authors>Cumhur Gülseren</Authors>
    <Company>Onventus</Company>
    <Description>Ultra-fast bulk insert and update library for PostgreSQL in .NET</Description>
    <RepositoryUrl>https://github.com/cumhurgulserens/PgBulkOps</RepositoryUrl>
    <RepositoryType>git</RepositoryType>

    <PackageTags>postgresql;bulk;insert;update;performance;npgsql</PackageTags>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageReadmeFile>README.md</PackageReadmeFile>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Npgsql" Version="9.0.4" />
    <None Include="README.md" Pack="true" PackagePath="\" />
  </ItemGroup>

</Project>
